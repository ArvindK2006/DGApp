<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    tools:context="MapsFragment"
    tools:layout_editor_absoluteX="7dp"
    tools:layout_editor_absoluteY="3dp">

    <fragment
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="356dp"
        android:layout_height="344dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:layout_marginRight="20dp"
        android:layout_marginBottom="250dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.285" />

    <Button
        android:id="@+id/defaultMap"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_marginLeft="35dp"
        android:layout_marginTop="40dp"
        android:text="Map"
        android:textColor="@drawable/button_text_color"
        app:backgroundTint="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/map"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.014"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/satelliteMap"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:layout_marginTop="40dp"
        android:text="Satellite"
        android:textColor="@drawable/button_text_color"
        app:backgroundTint="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/map"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.374"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <CheckBox
        android:id="@+id/PicnicEnable"
        android:layout_width="124dp"
        android:layout_height="40dp"
        android:layout_marginLeft="220dp"
        android:layout_marginBottom="210dp"
        android:buttonTint="@color/dark_Green"
        android:checked="true"
        android:text="Picnic Areas"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.966" />

    <CheckBox
        android:id="@+id/UnpavedTrailsEnabled"
        android:layout_width="130dp"
        android:layout_height="40dp"
        android:layout_marginLeft="220dp"
        android:layout_marginBottom="60dp"
        android:buttonTint="@color/dark_Green"
        android:checked="true"
        android:text="Unpaved Trails"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.974" />

    <CheckBox
        android:id="@+id/ParkingEnable"
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginBottom="160dp"
        android:buttonTint="@color/dark_Green"
        android:checked="true"
        android:text="Parking"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.967" />

    <CheckBox
        android:id="@+id/SleddingHillEnable"
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginBottom="60dp"
        android:buttonTint="@color/dark_Green"
        android:checked="true"
        android:text="Sledding"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.974" />

    <CheckBox
        android:id="@+id/FlyingAreaEnabled"
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginBottom="10dp"
        android:buttonTint="@color/dark_Green"
        android:checked="true"
        android:text="Flying Area"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.978" />

    <CheckBox
        android:id="@+id/LakeEnable"
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:layout_marginLeft="220dp"
        android:layout_marginBottom="10dp"
        android:buttonTint="@color/dark_Green"
        android:checked="true"
        android:text="Lake"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.978" />

    <CheckBox
        android:id="@+id/CampEnabled"
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginBottom="110dp"
        android:buttonTint="@color/dark_Green"
        android:checked="true"
        android:text="Camps"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.971" />

    <CheckBox
        android:id="@+id/TrailAccessEnabled"
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:layout_marginLeft="220dp"
        android:layout_marginBottom="160dp"
        android:buttonTint="@color/dark_Green"
        android:checked="true"
        android:text="Trail Access"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.967" />

    <CheckBox
        android:id="@+id/PavedTrailsEnabled"
        android:layout_width="130dp"
        android:layout_height="40dp"
        android:layout_marginLeft="220dp"
        android:layout_marginBottom="110dp"
        android:buttonTint="@color/dark_Green"
        android:checked="true"
        android:text="Paved Trails"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.971" />

    <CheckBox
        android:id="@+id/EntranceEnable"
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginBottom="210dp"
        android:buttonTint="@color/dark_Green"
        android:checked="true"
        android:text="Entrance"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.966" />

</androidx.constraintlayout.widget.ConstraintLayout>

